{
    "manifest_version": 3,
    "name": "WebPrarie",
    "version": "1.0",
    "description": "Changes PrarieLearn's progress bars to match WebWork more closely and improves the UI to be more readable.",
    "content_scripts": [
        {
            "js": [
                "scripts/content.js"
            ],
            "matches": [
                "https://us.prairielearn.com/*",
                "https://ca.prairielearn.com/*"
            ]
        }
    ]
}